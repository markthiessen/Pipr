<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Copyright>Copyright Mark Thiessen</Copyright>
    <Title>Pipr</Title>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Pipr</PackageId>
    <PackageDescription>A simple chain of responsibility framework for .NET</PackageDescription>
    <PackageTags>chain of responsibility;pipeline;</PackageTags>
    <Version>1.1.0</Version>
    <Authors>Mark Thiessen</Authors>
    <Company>Mark Thiessen</Company>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/markthiessen/Pipr</PackageProjectUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.2.0" />
  </ItemGroup>
  <ItemGroup>
    <None Include="LICENSE.txt" Pack="true" PackagePath="$(PackageLicenseFile)"/>
  </ItemGroup>
</Project>